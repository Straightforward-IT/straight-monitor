{
  "scripts": {
    "install:frontend": "cd frontend/Straight-Monitor && npm install",
    "install:all": "npm install && npm run install:frontend",
    "build": "cd frontend/Straight-Monitor && npm install && npm run build",
    "frontend": "npm run dev --prefix frontend/Straight-Monitor",
    "backend": "nodemon --cwd api app.js",
    "dev": "concurrently \"npm run frontend\" \"npm run backend\" \"cloudflared tunnel run dev-backend\""
  },
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^7.0.1",
    "concurrently": "^9.0.1",
    "file-saver": "^2.0.5",
    "lodash.debounce": "^4.0.8",
    "nodemon": "^3.1.7",
    "pdfmake": "^0.2.18",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7"
  }
}
